{
  "name": "@laserware/arcade",
  "version": "2.2.2",
  "description": "Miscellaneous utility functions.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "git+https://github.com/laserware/arcade.git",
    "type": "git"
  },
  "author": "Mike Rourke <mike.w.rourke@gmail.com>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "utilities"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "check:format": "prettier --check src",
    "check:types": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "preversion": "pnpm lint && pnpm check:types",
    "version": "pnpm format && git add -A src",
    "postversion": "git push && git push --tags",
    "format": "prettier --write src",
    "lint": "eslint --cache src",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@types/lodash.isequal": "^4.5.8",
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "@laserware/eslint-config": "^1.2.1",
    "@rushstack/eslint-patch": "^1.7.2",
    "@types/node": "^20.11.7",
    "@vitest/coverage-istanbul": "^1.2.2",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "prettier": {
    "trailingComma": "all"
  }
}
