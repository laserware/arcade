{
  "name": "@laserware/arcade",
  "version": "3.12.1",
  "description": "Miscellaneous utility functions.",
  "author": "Mike Rourke <mike.w.rourke@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/laserware/arcade.git",
    "type": "git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "utilities"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup && cd dist && bunx prettier --write .",
    "prepublishOnly": "bun run build",
    "preversion": "bun run lint && bun run typecheck",
    "version": "bun run format && git add -A src",
    "postversion": "git push && git push --tags",
    "docs:generate": "typedoc  --options typedoc.json",
    "docs:serve": "mkdir -p ./site && cd ./site && ../node_modules/.bin/vite",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write src",
    "lint": "biome lint src",
    "test": "bun test"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "latest",
    "@types/node": "^22",
    "tsup": "^8.3.5",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.2"
  }
}
