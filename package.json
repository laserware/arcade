{
  "name": "@laserware/arcade",
  "version": "2.4.0",
  "description": "Miscellaneous utility functions.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.cjs"
    }
  },
  "repository": "github:laserware/arcade.git",
  "bugs": "https://github.com/laserware/arcade/issues",
  "author": "Mike Rourke <mike.w.rourke@gmail.com>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "utilities"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup && cd dist && npx prettier --write index.d.ts",
    "prepublishOnly": "npm run build",
    "preversion": "npm run eslint && npm run typecheck",
    "version": "npm run prettier && git add -A src",
    "postversion": "git push && git push --tags",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --cache --write src",
    "prettier:ci": "prettier --check src",
    "eslint": "eslint --cache src",
    "vitest": "vitest"
  },
  "dependencies": {
    "@types/lodash.isequal": "^4.5.8",
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "@laserware/eslint-config": "^2.0.0",
    "@rushstack/eslint-patch": "^1.7.2",
    "@types/node": "^20.11.19",
    "@vitest/coverage-istanbul": "^1.3.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "prettier": {
    "trailingComma": "all"
  }
}
